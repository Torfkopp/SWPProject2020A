    <Button fx:id="buyEntwicklungskarteButton" layoutX="452.0" layoutY="285.0" mnemonicParsing="false"
            onAction="#onBuyEntwicklungskarteButtonPressed" text="Buy Card"/>
    <Button fx:id="cancelButton" layoutX="452.0" layoutY="341.0" mnemonicParsing="false"
