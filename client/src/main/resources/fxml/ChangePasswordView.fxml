<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="de.uol.swp.client.ChangePassword.ChangePasswordPresenter"
            prefHeight="400.0" prefWidth="600.0">


    <PasswordField id="OldPasswordField" fx:id="OldPasswordField1" layoutX="142.0" layoutY="25.0" promptText="Enter old Password"/>
    <PasswordField id="NewPasswordField" fx:id="NewPasswordField1" layoutX="142.0" layoutY="55.0" promptText="Enter new Password"/>
    <PasswordField id="NewPasswordField" fx:id="NewPasswordField2" layoutX="142.0" layoutY="85.0" promptText="Repeat new Password"/>

    <Button id="ChangePasswordButton" defaultButton="true" onAction="#onChangePasswordButtonPressed"  layoutX="112.0" layoutY="144.0" mnemonicParsing="false" text="Change"/>
    <Button id="CancelChangeButton" layoutX="203.0" layoutY="144.0" mnemonicParsing="false" onAction="#onCancelButtonPressed" text="Cancel"/>

    <Label layoutX="44.0" layoutY="30.0" text="Old Password"/>
    <Label layoutX="44.0" layoutY="60.0" text="New Password"/>
    <Label layoutX="44.0" layoutY="90.0" text="New Password"/>
</AnchorPane>

