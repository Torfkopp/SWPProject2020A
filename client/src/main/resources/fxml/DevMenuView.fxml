<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="de.uol.swp.client.devmenu.DevMenuPresenter">
    <ListView fx:id="classListView" layoutX="14.0" layoutY="14.0" prefHeight="175.0" prefWidth="200.0"/>
    <ListView fx:id="constructorList" layoutX="224.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0">
        <placeholder>
            <Label text="%devmenu.lists.constructors.placeholder"/>
        </placeholder>
    </ListView>
    <HBox fx:id="parameterBox" layoutX="14.0" layoutY="221.0" prefHeight="100.0" prefWidth="410.0">
        <spacing>5</spacing>
    </HBox>
    <TextField fx:id="classFilterTextField" layoutX="14.0" layoutY="188.0" prefHeight="25.0" prefWidth="200.0"/>
    <VBox alignment="TOP_CENTER" layoutX="424.0" layoutY="14.0" prefHeight="200.0" prefWidth="183.0">
        <Button mnemonicParsing="false" onAction="#onSendButtonPressed" text="%devmenu.buttons.send"
                VBox.vgrow="ALWAYS"/>
        <Label maxHeight="Infinity" maxWidth="Infinity" text="%devmenu.labels.legend"
               VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
        </Label>
    </VBox>
</AnchorPane>
