<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="196.0" prefWidth="407.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="de.uol.swp.client.ChangePassword.PasswordConfirmationPresenter">

    <PasswordField id="ConfirmPassword" fx:id="ConfirmPassword" layoutX="142.0" layoutY="25.0"
                   promptText="%changeaccdetails.promts.confirmpw"/>

    <Button id="ConfirmPasswordButton" defaultButton="true" layoutX="112.0" layoutY="144.0" mnemonicParsing="false"
            onAction="#onConfirmPasswordButtonPressed" text="%changeaccdetails.buttons.confirm"/>
    <Button id="CancelChangeButton" layoutX="203.0" layoutY="144.0" mnemonicParsing="false"
            onAction="#onCancelButtonPressed" text="%changeaccdetails.button.cancel"/>

    <Label layoutX="44.0" layoutY="30.0" text="%changepw.labels.oldpw"/>
</AnchorPane>
