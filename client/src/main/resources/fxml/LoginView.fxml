<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="196.0" prefWidth="407.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="de.uol.swp.client.auth.LoginPresenter">
    <Label layoutX="43.0" layoutY="39.0" text="%login.labels.login"/>
    <TextField id="loginField" fx:id="loginField" layoutX="142.0" layoutY="34.0" promptText="%login.prompts.username">
        <tooltip>
            <Tooltip text="%login.tooltips.login"/>
        </tooltip>
    </TextField>
    <PasswordField id="passwordField" fx:id="passwordField" layoutX="142.0" layoutY="78.0"
                   promptText="%login.prompts.password"/>
    <Button id="loginButton" defaultButton="true" layoutX="116.0" layoutY="133.0"
            mnemonicParsing="false" onAction="#onLoginButtonPressed" text="%login.buttons.login"/>
    <Button id="registerButton" layoutX="192.0" layoutY="133.0" mnemonicParsing="false"
            onAction="#onRegisterButtonPressed" text="%login.buttons.register"/>
    <Label layoutX="44.0" layoutY="83.0" text="%login.labels.password"/>
</AnchorPane>
